{
    "name": "forumhulp/uploadstyle",
    "type": "phpbb-extension",
    "description": "Upload Styles enables you to upload style zip files or delete style folders from the server. With this extension you can install/update/delete styles without using FTP.",
    "homepage": "http://forumhulp.com",
    "version": "3.1.1",
    "keywords": ["style", "forumhulp", "phpbb", "extension", "upload", "unzip", "zip"],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "John Peskens",
            "email": "info@forumhulp.com",
            "homepage": "http://forumhulp.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpbb/phpbb": "3.1.*@dev"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Upload Style",
        "soft-require": {
           "phpbb/phpbb": "3.1.*@dev"
        },
        "version-check": {
            "host": "forumhulp.com",
            "directory": "/ext",
            "filename": "index.php?id=37"
        }
    }
}
